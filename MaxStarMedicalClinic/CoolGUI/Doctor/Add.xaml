        <TextBlock Height="23" HorizontalAlignment="Left" Margin="8,25,0,0" Name="textBlock1" Text="Patient ID:" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="63,89,0,0" Name="Patient_Name" VerticalAlignment="Top" Width="84" IsReadOnly="True" TextChanged="Patient_Name_TextChanged" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="23,90,0,0" Name="textBlock2" Text="Name:" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Right" IsReadOnly="True" Margin="0,89,266,0" VerticalAlignment="Top" Width="78" Name="Last_Name" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="177,89,0,0" Name="textBlock3" Text="Last_Name:" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" IsReadOnly="True" Margin="416,88,0,0" Name="Age" VerticalAlignment="Top" Width="82" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="386,90,0,0" Name="textBlock4" Text="Age:" VerticalAlignment="Top" />
