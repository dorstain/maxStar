        <TextBlock Height="23" HorizontalAlignment="Left" Margin="16,132,0,0" Name="textBlock5" Text="Gender:" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" IsReadOnly="True" Margin="265,131,0,0" Name="textBox4" VerticalAlignment="Top" Width="78" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="225,132,0,0" Name="textBlock6" Text="Date:" VerticalAlignment="Top" />
        <Button Content="Search!" Height="23" HorizontalAlignment="Left" Margin="194,25,0,0" Name="Search" VerticalAlignment="Top" Width="75" Click="Search_PatientID" />
    </Grid>
</Window>
